{
  "name": "@storybook/react-simple-di",
  "version": "1.3.0",
  "description": "Simple Dependancy Injection Solution for React",
  "repository": {
    "type": "git",
    "url": "https://github.com/storybooks/react-simple-di/"
  },
  "license": "MIT",
  "options": {
    "mocha": "--require scripts/mocha_runner lib/**/__tests__/**/*.js"
